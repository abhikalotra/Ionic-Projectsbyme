<!doctype html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
        <title>Diesel Cult</title>
        <!-- Custom CSS -->
        <link rel="stylesheet" href="assets/css/custom.css">
        <script type="text/javascript" charset="utf-8" src="cordova.js"></script>
        <script src="assets/js/jquery-1.4.4.min.js"></script>
        <script type="text/javascript" charset="utf-8">
            document.addEventListener('backbutton', onBackKeyDown, false);
            function onBackKeyDown() {
                window.location = "category.html";
            }
            $(function () {
                $("#profile").click(function () {
                    var networkState = navigator.connection.type;
                    if (networkState == "none")
                        window.plugins.toast.show('No internet connection.', 'short', 'bottom');
                    else {
                        window.location = "profile.html";
                    }
                })
            });

        </script>   
        <script>
            function closePopup()
            {
                $('#divId').fadeOut();
            }
            function showDiv()
            {
                $('#divId').fadeIn();
            }
        </script>
    </head>

    <body style="background:#f5f4f0;">


        <div style="display:none;" id="divId" class="model_black_area">
            <div style="height:197px;" class="model_black_area_white">
                <p>Are you sure you want to logout?</p>
                <a href="login.html"><button class="enter_button">Yes</button></a>
                <a onclick="closePopup()" href="#"><button style="margin-top:10px;" class="enter_button">No</button></a>
            </div><!-- model_black_area_white close -->
        </div><!-- model_black_area close -->



        <header class="header_main">
            <div class="header_inner">

                <div class="header_inner_text_upper">
