            <a href="feedback.html" ><div class="more_category_one"><div class="more_category_one_inner"><img src="assets/images/offers.png"><br>Feedback</div><!-- more_category_one_inner close --></div></a><!-- more_category_one close -->
            <a href="contact.html" ><div class="more_category_one"><div class="more_category_one_inner"><img src="assets/images/messages.png"><br>Contact us</div><!-- more_category_one_inner close --></div></a><!-- more_category_one close -->
